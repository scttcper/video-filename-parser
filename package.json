{
  "name": "@ctrl/video-filename-parser",
  "version": "0.0.0-placeholder",
  "description": "A radarr style release name parser",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/video-filename-parser",
  "homepage": "https://video-filename-parser.vercel.app",
  "type": "module",
  "exports": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "keywords": [
    "scene",
    "release",
    "radarr",
    "sonarr"
  ],
  "scripts": {
    "demo:watch": "snowpack dev",
    "demo:build": "snowpack build",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc",
    "test": "ava",
    "test:watch": "ava --watch",
    "test:ci": "c8 --reporter=text --reporter=lcov ava"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "2.0.13",
    "@sindresorhus/tsconfig": "^2.0.0",
    "@snowpack/plugin-postcss": "1.4.3",
    "@tailwindcss/forms": "0.3.3",
    "@types/node": "16.7.13",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "autoprefixer": "10.3.4",
    "ava": "4.0.0-alpha.2",
    "c8": "^7.8.0",
    "cssnano": "5.0.8",
    "expect": "27.1.0",
    "postcss": "8.3.6",
    "postcss-cli": "8.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "snowpack": "3.8.8",
    "tailwindcss": "2.2.11",
    "ts-node": "10.2.1",
    "typescript": "4.4.2"
  },
  "ava": {
    "files": [
      "test/**/*.spec.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=15"
  }
}
