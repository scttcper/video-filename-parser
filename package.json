{
  "name": "@ctrl/video-filename-parser",
  "version": "0.0.0-placeholder",
  "description": "A radarr style release name parser",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/video-filename-parser",
  "homepage": "https://video-filename-parser.vercel.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "scene",
    "release",
    "radarr",
    "sonarr",
    "typescript"
  ],
  "scripts": {
    "demo:watch": "npm run demo:watch --workspace=demo",
    "demo:build": "npm run demo:build --workspace=demo",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.14.0",
    "@babel/preset-typescript": "7.13.0",
    "@ctrl/eslint-config": "2.0.6",
    "@jest/globals": "26.6.2",
    "@types/node": "15.3.1",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "typescript": "4.2.4"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=12"
  },
  "workspaces": [
    "demo"
  ]
}
