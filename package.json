{
  "name": "@ctrl/video-filename-parser",
  "version": "0.0.0-placeholder",
  "description": "A radarr style release name parser",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/video-filename-parser",
  "homepage": "https://video-filename-parser.vercel.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "scene",
    "release",
    "radarr",
    "sonarr",
    "typescript"
  ],
  "scripts": {
    "demo:watch": "snowpack dev",
    "demo:build": "snowpack build",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/preset-typescript": "7.12.7",
    "@ctrl/eslint-config": "1.2.12",
    "@fullhuman/postcss-purgecss": "4.0.0",
    "@snowpack/plugin-postcss": "1.1.0",
    "@snowpack/plugin-webpack": "2.3.0",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/ui": "0.7.2",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "autoprefixer": "10.2.4",
    "cssnano": "4.1.10",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "path-browserify": "1.0.1",
    "postcss": "8.2.4",
    "postcss-cli": "8.3.1",
    "preact": "10.5.12",
    "snowpack": "3.0.11",
    "snowpack-plugin-hash": "0.14.0",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=12"
  }
}
