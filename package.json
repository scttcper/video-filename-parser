{
  "name": "@ctrl/video-filename-parser",
  "version": "0.0.0-placeholder",
  "description": "A radarr style release name parser",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/video-filename-parser",
  "homepage": "https://video-filename-parser.vercel.app",
  "type": "module",
  "exports": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "keywords": [
    "scene",
    "release",
    "radarr",
    "sonarr"
  ],
  "scripts": {
    "demo:build": "npm run build --workspace=demo",
    "demo:watch": "npm run dev --workspace=demo",
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit --outputFile=./junit.xml"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.6.3",
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/node": "18.16.0",
    "@vitest/coverage-c8": "0.30.1",
    "c8": "7.13.0",
    "typescript": "5.0.4",
    "vitest": "0.30.1"
  },
  "workspaces": [
    "demo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "engines": {
    "node": ">=14"
  }
}
