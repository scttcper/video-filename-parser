{
  "name": "@ctrl/video-filename-parser",
  "version": "0.0.0-placeholder",
  "description": "A radarr style release name parser",
  "keywords": [
    "radarr",
    "release",
    "scene",
    "sonarr"
  ],
  "homepage": "https://video-filename-parser.ep.workers.dev",
  "license": "MIT",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "repository": "scttcper/video-filename-parser",
  "workspaces": [
    "demo"
  ],
  "files": [
    "dist/src"
  ],
  "type": "module",
  "sideEffects": false,
  "types": "./dist/src/index.d.ts",
  "exports": "./dist/src/index.js",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "demo:build": "pnpm run -r build",
    "demo:watch": "pnpm run -r dev",
    "lint": "oxlint . && oxfmt --check",
    "lint:fix": "oxlint . --fix && oxfmt",
    "prepare": "pnpm run build",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@ctrl/oxlint-config": "1.4.0",
    "@sindresorhus/tsconfig": "8.1.0",
    "@types/node": "25.2.3",
    "oxfmt": "0.33.0",
    "oxlint": "1.48.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.29.3"
}
